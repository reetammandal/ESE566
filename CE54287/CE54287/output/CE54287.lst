05EB: 67       ASR   A                  (0262)     asr   A
05EC: 67       ASR   A                  (0263)     asr   A
05ED: 67       ASR   A                  (0264)     asr   A
05EE: 21 0F    AND   A,0xF              (0265)     and   A,0Fh                        ; Mask off nibble
